			<comm_type>special</comm_type>
			<parameter>
				<id>bw_fni_phit</id>
				<value>BW_FNI_PHIT</value>
			</parameter>
			<parameter>
				<id>bw_bni_phit</id>
				<value>BW_BNI_PHIT</value>
			</parameter>
