		<def_parameter>
			<name>BW_BNI_PHIT</name>
			<value>8</value>
		</def_parameter>

		<signal>
			<name>clk</name>
			<width>1</width>
			<port>input</port>
		</signal>

		<signal>
			<name>rstnn</name>
			<width>1</width>
			<port>input</port>
		</signal>

		<interface>
