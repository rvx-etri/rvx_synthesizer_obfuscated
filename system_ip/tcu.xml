<?xml version="1.0" ?>
<rvx>
	<dec_ip>
		<type>cpu_tcu</type>
		<name>temporary_caching_unit</name>
		<hdl_name>ERVP_TEMPORARY_CACHING_UNIT</hdl_name>
		<def_parameter>
			<name>BW_ADDR</name>
			<value>1</value>
		</def_parameter>
		<def_parameter>
			<name>BW_DATA</name>
			<value>1</value>
		</def_parameter>
		<def_parameter>
			<name>BW_AXI_TID</name>
			<value>1</value>
		</def_parameter>

		<signal>
			<name>clk</name>
			<width>1</width>
			<port>input</port>
			<comm_type>#</comm_type>
		</signal>

		<signal>
			<name>rstnn</name>
			<width>1</width>
			<port>input</port>
		</signal>

		<reset_property>
			<id>rstnn</id>
			<type>asynch</type>
			<type>negative</type>
			<group>1</group>
		</reset_property>

		<core>
			<sync>clk</sync>
			<init>rstnn</init>
		</core>

		<interface>
			<name>from_core</name>
			<library_name>axi</library_name>
			<hdl_prefix></hdl_prefix>
			<hdl_suffix>_x</hdl_suffix>
			<role>receiver</role>
			<comm_type>special</comm_type>
			<parameter>
				<id>bw_addr</id>
				<value>BW_ADDR</value>
			</parameter>
			<parameter>
				<id>bw_data</id>
				<value>BW_DATA</value>
			</parameter>
			<parameter>
				<id>bw_tid</id>
				<value>BW_AXI_TID</value>
			</parameter>
			<sync>clk</sync>
			<init>rstnn</init>
		</interface>

		<interface>
			<name>to_noc</name>
			<library_name>axi</library_name>
			<hdl_prefix></hdl_prefix>
			<hdl_suffix>_y</hdl_suffix>
			<role>sender</role>
			<parameter>
				<id>bw_addr</id>
				<value>BW_ADDR</value>
			</parameter>
			<parameter>
				<id>bw_data</id>
				<value>BW_DATA</value>
			</parameter>
			<parameter>
				<id>bw_tid</id>
				<value>BW_AXI_TID</value>
			</parameter>
			<sync>clk</sync>
			<init>rstnn</init>
		</interface>

		<interface>
			<name>control</name>
			<library_name>apb</library_name>
			<hdl_prefix></hdl_prefix>
			<role>receiver</role>
			<comm_type>tcu_control_#</comm_type>
			<memorymap>
				<size>0x10000</size>
			</memorymap>
			<parameter>
				<id>bw_addr</id>
				<value>BW_ADDR</value>
			</parameter>
			<parameter>
				<id>bw_data</id>
				<value>32</value>
			</parameter>
			<sync>clk</sync>
			<init>rstnn</init>
		</interface>

	</dec_ip>
</rvx>
