<?xml version="1.0" ?>
<rvx>
	<dec_ip>
		<name>prvp_c2c_slave01</name>
    <hdl_name>PRVP_C2C_SLAVE01</hdl_name>
		<def_parameter>
			<name>BW_ADDR</name>
			<value>32</value>
		</def_parameter>
		<def_parameter>
			<name>BW_AXI_DATA</name>
			<value>32</value>
		</def_parameter>
		<def_parameter>
			<name>BW_AXI_TID</name>
			<value>4</value>
		</def_parameter>

    <signal>
			<name>clk_axi</name>
			<width>1</width>
			<port>input</port>
			<comm_type>clk_network</comm_type>
		</signal>

		<signal>
			<name>rstnn_axi</name>
			<width>1</width>
			<port>input</port>
		</signal>

		<reset_property>
			<id>rstnn_axi</id>
			<type>asynch</type>
			<type>negative</type>
			<group>2</group>
		</reset_property>

		<core>
			<sync>clk_axi</sync>
			<init>rstnn_axi</init>
    </core>

    <interface>
			<name>master</name>
			<library_name>axi4</library_name>
			<hdl_prefix></hdl_prefix>
			<role>sender</role>
			<comm_type>memory:all</comm_type>
			<parameter>
				<id>bw_addr</id>
				<value>BW_ADDR</value>
			</parameter>
			<parameter>
				<id>bw_data</id>
				<value>BW_AXI_DATA</value>
			</parameter>
			<parameter>
				<id>bw_tid</id>
				<value>BW_AXI_TID</value>
			</parameter>
			<sync>clk_axi</sync>
			<init>rstnn_axi</init>
		</interface>

    <signal>
      <name>c2cs01_clk</name>
      <width>1</width>
      <port>input</port>
      <comm_type>external</comm_type>
    </signal>
    <signal>
      <name>c2cs01_csn</name>
      <width>1</width>
      <port>input</port>
      <comm_type>external</comm_type>
    </signal>
    <signal>
      <name>c2cs01_sdo</name>
      <width>4</width>
      <port>output</port>
      <comm_type>external</comm_type>
    </signal>
    <signal>
      <name>c2cs01_sdi</name>
      <width>4</width>
      <port>input</port>
      <comm_type>external</comm_type>
    </signal>
	</dec_ip>
</rvx>

