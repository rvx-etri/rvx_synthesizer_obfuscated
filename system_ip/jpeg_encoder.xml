<?xml version="1.0" ?>
<rvx>
	<dec_ip>
		<name>jpeg_encoder</name>
		<hdl_name>ERVP_JPEG_ENCODER</hdl_name>
		<def_parameter>
			<name>BW_ADDR</name>
			<value>32</value>
		</def_parameter>
		<def_parameter>
			<name>BW_DATA</name>
			<value>64</value>
		</def_parameter>
		<def_parameter>
			<name>BW_AXI_TID</name>
			<value>4</value>
		</def_parameter>

		<signal>
			<name>clk</name>
			<width>1</width>
			<port>input</port>
			<comm_type>gclk_jpeg</comm_type>
		</signal>

		<signal>
			<name>rstnn</name>
			<width>1</width>
			<port>input</port>
		</signal>

		<reset_property>
			<id>rstnn</id>
			<type>asynch</type>
			<type>negative</type>
			<group>2</group>
		</reset_property>

		<core>
			<sync>clk</sync>
			<init>rstnn</init>
		</core>

		<signal>
			<name>clk_axi</name>
			<width>1</width>
			<port>input</port>
			<comm_type>clk_network</comm_type>
		</signal>

		<signal>
			<name>rstnn_axi</name>
			<width>1</width>
			<port>input</port>
			<comm_type>rstnn_noc</comm_type>
		</signal>

		<signal>
			<name>clk_ahb</name>
			<width>1</width>
			<port>input</port>
			<comm_type>clk_network</comm_type>
		</signal>

		<signal>
			<name>rstnn_ahb</name>
			<width>1</width>
			<port>input</port>
			<comm_type>rstnn_noc</comm_type>
		</signal>

		<interface>
			<name>master</name>
			<library_name>axi</library_name>
			<hdl_prefix></hdl_prefix>
			<role>sender</role>
			<comm_type>memory:all</comm_type>
			<parameter>
				<id>bw_addr</id>
				<value>BW_ADDR</value>
			</parameter>
			<parameter>
				<id>bw_data</id>
				<value>BW_DATA</value>
			</parameter>
			<parameter>
				<id>bw_tid</id>
				<value>BW_AXI_TID</value>
			</parameter>
			<sync>clk_axi</sync>
			<init>rstnn_axi</init>
		</interface>

		<interface>
			<name>slave</name>
			<library_name>ahbs</library_name>
			<hdl_prefix></hdl_prefix>
			<role>receiver</role>
			<comm_type>memory:all</comm_type>
			<memorymap>
				<size>0x1000</size>
			</memorymap>
			<parameter>
				<id>bw_addr</id>
				<value>BW_ADDR</value>
			</parameter>
			<parameter>
				<id>bw_data</id>
				<value>32</value>
			</parameter>
			<sync>clk_ahb</sync>
			<init>rstnn_ahb</init>
		</interface>
	</dec_ip>
</rvx>
