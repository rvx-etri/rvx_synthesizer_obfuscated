<?xml version="1.0" ?>
<rvx>
	<def_interface>
		<name>valid_ready_data</name>
		<hdl_prefix></hdl_prefix>
		<role>sender</role>
		<def_parameter>
			<name>bw_data</name>
			<value>32</value>
    </def_parameter>

    <signal>
      <name>valid</name>
      <width>1</width>
      <port>output</port>
    </signal>
    <signal>
      <name>ready</name>
      <width>1</width>
      <port>input</port>
    </signal>
    <signal>
      <name>data</name>
      <width>bw_data</width>
      <port>output</port>
    </signal>

	</def_interface>
</rvx>
